<div id="content" class="span10">
			<!-- content starts -->
			<div>
				<ul class="breadcrumb">
					<li>
						<%= link_to "添加车系", new_brand_series_path(@brand), :class => "btn btn-info" %>
					</li>
					<li>
						<%= link_to "返回品牌列表", :brands, :class => "btn btn-info" %>
					</li>
				</ul>
			</div>
      <%= render partial: "layouts/notice", locals: { model: nil} %>      
			<div class="row-fluid sortable">
				<div class="box span12">
					<div class="box-header well" data-original-title>
						<h2><%= @brand.name %>-车系</h2>
					</div>
					<div class="box-content">
            <table class="table table-striped table-bordered bootstrap-datatable">
						  <thead>
							  <tr>
								  <th>车系名称</th>
                  <th>操作</th>
							  </tr>
						  </thead>   
						  <tbody>
                <%= render partial: "item", collection: @series, as: :series %>
						  </tbody>
					  </table>
					</div>
				</div><!--/span-->			
			</div><!--/row-->    
					<!-- content ends -->
			</div><!--/#content.span10-->