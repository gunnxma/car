<div id="content" class="span10">
			<!-- content starts -->
			<div>
				<ul class="breadcrumb">
					<li>
            <%= link_to "添加", :new_price_ref, :class => "btn btn-info" %>
					</li>
				</ul>
			</div>
      <%= render partial: "layouts/notice", locals: { model: nil} %>      
			<div class="row-fluid">
				<div class="box span12">
					<div class="box-header well" data-original-title>
						<h2>历史成交</h2>
					</div>
					<div class="box-content">
            <%= search_form_for @q, :url => :price_refs, :method => "get" do |f| %>
              <%= render partial: "search_form", locals: {f: f} %>
            <% end %>
            <table class="table table-striped table-bordered bootstrap-datatable">
						  <thead>
							  <tr>
								  <th>品牌</th>
                  <th>年份</th>
                  <th>车型</th>
                  <th>指导价</th>
                  <th>优惠</th>
                  <th>重要配置</th>
                  <th>收购参考价</th>
                  <th>历史成交价</th>
                  <th>操作</th>
							  </tr>
						  </thead>   
						  <tbody>
                <%= render partial: "item", collection: @price_refs, as: :price_ref %>
						  </tbody>
					  </table> 
            <%= page_entries_info @price_refs %>
            <%= will_paginate @price_refs, renderer: SemanticPagination %>
					</div>
				</div><!--/span-->			
			</div><!--/row-->
					<!-- content ends -->
			</div><!--/#content.span10-->