<div id="content" class="span10">
			<!-- content starts -->
			<div>
				<ul class="breadcrumb">
					<li>
            <% # link_to "导出Excel", selloff_index_path(format: "xls"), :class => "btn btn-info" %>
            <%= link_to "导出Excel", request.url.sub!("index","index.xls"), :class => "btn btn-info" %>
					</li>
				</ul>
			</div>
      <%= render partial: "layouts/notice", locals: { model: nil} %>      
			<div class="row-fluid">
				<div class="box span12">
					<div class="box-header well" data-original-title>
						<h2>已售车辆</h2>
					</div>
					<div class="box-content">
            <%= search_form_for @q, :url => selloff_index_path, :method => "get" do |f| %>
            <%= render partial: "search_form", locals: {f: f} %>
            <% end %>
            <table class="table table-striped table-bordered bootstrap-datatable">
						  <thead>
							  <tr>
								  <th>车辆编号</th>
								  <th>店面</th>
								  <th>vin码</th>
                  <th>操作</th>
                  <th>收购日期</th>
                  <th>售出日期</th>
                  <th>出厂日期</th>
                  <th>状态</th>
                  <th>类型</th>
                  <th>评估照片</th>
                  <th>车辆品牌型号</th>
                  <th>排量</th>
                  <th>表显里程</th>
                  <th>跟进</th>
                  <th>成交价格</th>
                  <th>销售限价</th>
                  <th>销售途径</th>
							  </tr>
						  </thead>   
						  <tbody>
                <%= render partial: "item", collection: @cars, as: :car %>
						  </tbody>
					  </table> 
            <%= page_entries_info @cars %>
            <%= will_paginate @cars, renderer: SemanticPagination %>
					</div>
				</div><!--/span-->			
			</div><!--/row-->
					<!-- content ends -->
			</div><!--/#content.span10-->