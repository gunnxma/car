<?xml version="1.0"?>
<Workbook xmlns="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:o="urn:schemas-microsoft-com:office:office"
  xmlns:x="urn:schemas-microsoft-com:office:excel"
  xmlns:ss="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:html="http://www.w3.org/TR/REC-html40">
  <Worksheet ss:Name="Sheet1">
    <Table>
      <Row>
        <Cell><Data ss:Type="String">车辆编号</Data></Cell>
        <Cell><Data ss:Type="String">车辆信息</Data></Cell>
        <Cell><Data ss:Type="String">VIN</Data></Cell>
        <Cell><Data ss:Type="String">成交人员</Data></Cell>
        <Cell><Data ss:Type="String">整备费用</Data></Cell>
        <Cell><Data ss:Type="String">付款合计</Data></Cell>
        <Cell><Data ss:Type="String">收款合计</Data></Cell>
        <Cell><Data ss:Type="String">利润</Data></Cell>
      </Row>
    <% @cars.each do |car| %>
      <Row>
        <Cell><Data ss:Type="String"><%= car.car_no %></Data></Cell>
        <Cell><Data ss:Type="String"><%= car.info %></Data></Cell>
        <Cell><Data ss:Type="String"><%= car.vin %></Data></Cell>
        <Cell><Data ss:Type="String">
          录入人：<%= car.user.name %><br>
          评估人：<%= car.car_assess.user.name if car.car_assess.user %><br>
          成交人：<%= car.sell_user.name %>
          </Data></Cell>
        <Cell><Data ss:Type="String"><%= car.repair_costs %>万</Data></Cell>
        <Cell><Data ss:Type="String"><%= car.payment_costs %>万</Data></Cell>
        <Cell><Data ss:Type="String"><%= car.proceeds_costs %>万</Data></Cell>
        <Cell><Data ss:Type="String"><%= car.profit_costs %>万</Data></Cell>
      </Row>
    <% end %>
    </Table>
  </Worksheet>
</Workbook>