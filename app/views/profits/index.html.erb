<div id="content" class="span10">
			<!-- content starts -->
			<div>
				<ul class="breadcrumb">
					<li>
            <% #link_to "导出Excel", profits_path(format: "xls"), :class => "btn btn-info" %>
            <%= link_to "导出Excel", request.url.sub!("profits","profits.xls"), :class => "btn btn-info" %>
					</li>
				</ul>
			</div>
      <%= render partial: "layouts/notice", locals: { model: nil} %>      
			<div class="row-fluid">
				<div class="box span12">
					<div class="box-header well" data-original-title>
						<h2>利润分析</h2>
					</div>
					<div class="box-content">
            <%= search_form_for @q, :url => :proceeds, :method => "get" do |f| %>
              <%= render partial: "search_form", locals: {f: f} %>
            <% end %>
            <table class="table table-striped table-bordered bootstrap-datatable">
						  <thead>
							  <tr>
								  <th>车辆编号</th>
                  <th>车辆信息</th>
                  <th>VIN</th>
                  <th>成交人员</th>
                  <th>整备费用</th>
                  <th>付款合计</th>
                  <th>收款合计</th>
                  <th>利润</th>
							  </tr>
						  </thead>   
						  <tbody>
                <%= render partial: "item", collection: @cars, as: :car %>
						  </tbody>
					  </table> 
            <%= page_entries_info @cars %>
            <%= will_paginate @cars, renderer: SemanticPagination %>            
					</div>
				</div><!--/span-->			
			</div><!--/row-->
					<!-- content ends -->
			</div><!--/#content.span10-->