<div id="content" class="span10">
			<!-- content starts -->
			<div>
				<ul class="breadcrumb">
					<li>
            <%= link_to "添加", car_infos_search_path, :class => "btn btn-info" %>
            <% #link_to "导出Excel", proceeds_path(format: "xls"), :class => "btn btn-info" %>
            <%= link_to "导出Excel", request.url.sub!("proceeds","proceeds.xls"), :class => "btn btn-info" %>
					</li>
				</ul>
			</div>
      <%= render partial: "layouts/notice", locals: { model: nil} %>
			<div class="row-fluid">
				<div class="box span12">
					<div class="box-header well" data-original-title>
						<h2>收款管理</h2>
					</div>
					<div class="box-content">
            <%= search_form_for @q, :url => :proceeds, :method => "get" do |f| %>
              <%= render partial: "search_form", locals: { f: f} %>
            <% end %>
            <table class="table table-striped table-bordered bootstrap-datatable">
						  <thead>
							  <tr>
                                <th>车辆编号</th>
                                <th>店面</th>
                  <th>操作</th>
                  <th>车辆信息</th>
                  <th>VIN</th>
                  <th>收购价</th>
                  <th>销售成交价</th>
                  <th>收款明细</th>
                  <th>金额合计</th>
                  <th>收款人</th>
                  <th>收款时间</th>
							  </tr>
						  </thead>
						  <tbody>
                <%= render partial: "item", collection: @proceeds, as: :proceed %>
						  </tbody>
					  </table>
            <%= page_entries_info @proceeds %>
            <%= will_paginate @proceeds, renderer: SemanticPagination %>
					</div>
				</div><!--/span-->
			</div><!--/row-->
					<!-- content ends -->
			</div><!--/#content.span10-->
