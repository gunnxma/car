客户名称:<%= f.search_field :name_cont, style: "width:120px" %>
是否VIP:<%= f.select :vip_eq, [{id: 0, name: '否'},{id: 1, name: '是'}].collect { |p| [ p[:name], p[:id] ] }, { include_blank: true }, style: "width:50px" %>
联系电话:<%= f.search_field :phone_cont, style: "width:120px" %>
            需求品牌:<%= f.select :customer_need_brand_cont, Brand.all.order(:initial).collect { |p| [ p.name_with_initial, p.name ] }, { include_blank: true }, { :style => "width:100px", :onchange => "get_options(this.value)" } %>车系:<%= f.select :customer_need_series_cont, @series ? @series.collect { |p| [ p.name_with_initial, p.name ] } : {}, { include_blank: true }, :class => "car_series", :style => "width:100px" %>
            需求价位:<%= f.search_field :customer_need_expectative_price_min_gteq, style: "width:50px" %> ~ <%= f.search_field :customer_need_expectative_price_max_lteq, style: "width:50px" %>万
              <%= f.submit "查询" %>
